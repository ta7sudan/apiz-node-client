{
  "name": "apiz-node-client",
  "version": "0.1.4",
  "description": "Node client for apiz",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
    "lint": "eslint src",
    "format": "prettier --config .prettierrc.js --write src/**/*.js test/**/*.js",
    "inspect": "jsinspect src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ta7sudan/apiz-node-client.git"
  },
  "keywords": [
    "Node",
    "client",
    "apiz"
  ],
  "author": "ta7sudan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ta7sudan/apiz-node-client/issues"
  },
  "files": [
    "src",
    "dist",
    "example",
    "types"
  ],
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "homepage": "https://github.com/ta7sudan/apiz-node-client#readme",
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "jsinspect": "^0.12.7",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3"
  },
  "dependencies": {
    "got": "^9.2.2"
  }
}
